{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["import", {
      "libraryName": "antd",
      "style": true // or 'css'
    }]
  ]
}
