{
  "name": "picc-web-ts",
  "version": "1.0.0",
  "description": "广东人保平台端",
  "private": true,
  "author": "Cao Weiliang",
  "scripts": {
    "clean": "trash dist .awcache",
    "dll": "cross-env NODE_ENV=production webpack --config webpack.config.dll.babel.js",
    "start": "yarn clean && yarn dll && cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.dev.babel.js",
    "build": "yarn clean && yarn dll && cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "profile": "yarn clean && yarn dll && cross-env NODE_ENV=production webpack --config webpack.config.profile.babel.js",
    "eslint": "eslint -f codeframe \"**/*.js\"",
    "tslint": "tslint -t codeFrame \"**/*.tsx\" \"**/*.d.ts\" -e \"node_modules/**\" -e \"**/__tests__/**\"",
    "stylelint": "stylelint \"**/*.css **/*.sass **/*.scss\"",
    "lint": "yarn eslint && yarn tslint && yarn stylelint",
    "lint-staged:js": "eslint --ext .js",
    "test": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.4.13",
    "@types/react-content-loader": "^3.1.3",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack-env": "^1.13.6",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "antd": "^3.4.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.2",
    "classnames": "^2.2.5",
    "dva": "^2.4.0",
    "dva-loading": "^2.0.5",
    "echarts": "^4.1.0",
    "esri-loader": "^2.3.0",
    "esri-loader-react": "^2.0.3",
    "less": "2.7.2",
    "lodash": "^4.17.10",
    "lodash-decorators": "^5.0.0",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "path-to-regexp": "^2.1.0",
    "qs": "^6.5.0",
    "react": "^16.2.0",
    "react-container-query": "^0.9.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-fittext": "^1.0.0",
    "react-images": "^0.5.17",
    "react-transition-group": "^2.3.1",
    "url-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/react-test-renderer": "^16.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.1.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.0",
    "offline-plugin": "^5.0.5",
    "postcss": "^7.0.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-test-renderer": "^16.5.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.0.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^3.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.tsx",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ]
  }
}
